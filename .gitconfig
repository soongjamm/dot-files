[user]
  name = soongjamm
  email = soongjamm.dev@gmail.com

[core]
  pager = delta

[interactive]
  diffFilter = delta --color-only

[delta]
  navigate = true    # use n and N to move between diff sections
  side-by-side  = true


    # delta detects terminal colors automatically; set one of these to disable auto-detection
    # dark true
    # light true

[merge]
  conflictstyle = diff3

[diff]
  colorMoved = default

[alias]
    a = add --all
    p = push
    ch = checkout
    mc = merge --continue
    ma = merge --abort
    rc = rebase --continue
    ra = rebase --abort
